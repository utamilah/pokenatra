<h2>New Pokemon</h2>

<form action="/pokemon" method="post">
  <label>Name:</label>
  <input name="name">

  <label>Photo:</label>
  <input name="img_url">

  <label>Type:</label>
  <input name="poke_type">

  <label>Combat Power:</label>
  <input name="<%= rand(800) %>">

  <input type="submit" value="Create">
</form>
